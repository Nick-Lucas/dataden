{
  "name": "mydata",
  "version": "1.0.0",
  "description": "Personal and Extensible Data Warehouse",
  "author": "Nick Lucas",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "yarn jest --watch",
    "lint": "yarn eslint '*/**/*.{js,ts,jsx,tsx}'",
