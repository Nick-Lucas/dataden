      "packages/*"
    ]
  },
  "dependencies": {
    "@mydata/build": "1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "wsrun": "^5.2.4"
  }
}
